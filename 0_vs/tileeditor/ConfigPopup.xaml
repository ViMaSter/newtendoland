<Window x:Class="tileeditor.ConfigPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tileeditor"
        mc:Ignorable="d"
        Title="ConfigPopup" Background="#282b2d" Height="200" Width="360">
    <Grid>
        <Button Name="OK" Content="OK" HorizontalAlignment="Left" Margin="10,132,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Name="Cancel" Content="Cancel" HorizontalAlignment="Left" Margin="270,132,0,0" VerticalAlignment="Top" Width="75"/>
        <Grid Name="CustomContent" Margin="10,10,5,45.5" />
    </Grid>
</Window>
